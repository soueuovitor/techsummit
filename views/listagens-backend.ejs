<% include partials/header-backend %>

        <!-- page content -->
        <div class="right_col" role="main">
          <div class="">
            <div class="page-title">
              <div class="title_left">
                <h3>Listagens</h3>
              </div>
            </div>

            <div class="clearfix"></div>

            <div class="row">

              <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Patrocínios</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>Nome</th>
                          <th>Contribuição</th>
                        </tr>
                      </thead>
                      <tbody>
  						          <% for (var p of patrocinadores) { %>
  							          <tr>
  								          <td><a href="/patrocinador/<%= p.idPatrocinadores %>"><%= p.nomeP %></a></td>
  								          <td><%= p.valor %></td>
  							          </tr>
  						          <%} %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>

			        <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Utilizadores</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <tr>
							<th>Username</th>
							<th>Nome</th>
							<th>Email</th>
						</tr>
                        </tr>
                      </thead>
                      <tbody>
  						   	<% for (var u of user) { %>
									<tr>
										<td><%= u.username %></td>
										<td><%= u.name %></td>
										<td><%= u.email %></td>
									</tr>
							<%} %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>

            <div class="row">
              <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Speakers</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table class="table table-striped">
                      <thead>
                        <tr>
							<th>Nome</th>
							<th>Telemovel</th>
							<th>Nif</th>
                        </tr>
                      </thead>
                      <tbody>
                        	<% for (var s of speaker) { %>
                          <tr>
                            <td><a href="/speaker/<%= s.idSpeaker %>"><%= s.nomeS %></a></td>
                            <td><%= s.telemovel %></td>
                            <td><%= s.nif %></td>
                          </tr>
                        <%} %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>

			  <div class="col-md-6 col-sm-6 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Sessões</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>Nome</th>
                          <th>Data</th>
                          <th>Duração</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var s of sessao) { %>
                          <tr>
                            <td><a href="/sessoes/<%= s.idsessoes %>"><%= s.nome %></a></td>
                            <td><%= s.dataSessao %></td>
                            <td><%= s.duracao %></td>
                          </tr>
                      <%} %>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>

            <div class="row">
              <div class="col-md-12 col-sm-6 col-xs-12">
                <div class="x_panel">
                  <div class="x_title">
                    <h2>Colaboradores</h2>
                    <ul class="nav navbar-right panel_toolbox">
                      <li><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                    </ul>
                    <div class="clearfix"></div>
                  </div>
                  <div class="x_content">
                    <table class="table table-striped">
                      <thead>
                        <tr>
                          <th>Nome</th>
                          <th>Email</th>
                          <th>Morada</th>
                          <th>Contacto</th>
                          <th>Função</th>
                          <th>Tipo</th>
                          <th>NIF</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for (var c of colaborador) { %>
                          <tr>
                            <td><a href="/colaborador/<%= c.idColaboradores %>"><%= c.nomeC %></a></td>
                            <td><%= c.email %></td>
                            <td><%= c.morada %></td>
                            <td><%= c.telemovel %></td>
                            <td><%= c.funcao %></td>
                            <td><%= c.tipo %></td>
                            <td><%= c.nif %></td>
                          </tr>
                          <%} %>
                      </tbody>
                    </table>
                    </div>
                  </div>
                </div>
              </div>
              <div class="clearfix"></div>
            </div>
          </div>
        </div>

        <!-- /page content -->
<% include partials/footer-backend %>
